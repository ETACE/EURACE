n>Used by household to stock received quantities</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>sold_quantities_per_mall</name>\r\n    <description>Used by firm to hold informations about sales </description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>sold_quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>stock_empty</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>estimated_demand</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>data_type_sales</name>\r\n    <description>Used for storing the sales of a mall in one period.</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>period</name>\r\n      <description>Number of elapsed periods.</description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>sales</name>\r\n      <description>Sales in that mall in the certain period.</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>sales_statistics</name>\r\n    <description>Used by firm to store sales in malls of last periods</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\n\t<variable>\r\n      <type>int</type>\r\n      <name>mall_region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>data_type_sales_array</type>\r\n      <name>sales</name>\r\n      <description>A lower array that stores the sales of the last periods for the mall. </description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>financing_capital</name>\r\n    <description></description>\r\n    <variables>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>financing_per_month</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>nr_periods_before_repayment</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>temporary_sales_statistics</name>\r\n    <description>This data type is used for building a temporray array of last sales in the malls. </description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>period</name>\r\n      <description>Number of periods elapsedsince the sales was archieved.</description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>sales</name>\r\n      <description>Sales in the mall.</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>mean_wage</name>\r\n     <description>Average wage paid by a firm</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>need