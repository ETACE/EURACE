<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">
<xmodel version="2" >
 <name>Eurace</name>
 <version>Post-STFC, Isolated UNIBI model, 02/07/2008</version>
 <description>Authors: Simon Coakley, Simon Gemkow, Philipp Harting, Sander van der Hoog</description>
 <models>
  <model>
   <file>Cons_Goods_UNIBI/model.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Credit_UNIBI/model-dummy.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Credit_GREQAM/model-dummy.xml</file>
   <enabled>false</enabled>
  </model>
  <model>
   <file>Financial_Management_UNIBI/model.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Inv_Goods_UNIBI/model.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Labour_UNIBI/model.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Statistical_Office_UNIBI/model.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Government_UNIBI/model.xml</file>
   <enabled>false</enabled>
  </model>
  <model>
   <file>Government_GREQAM/model.xml</file>
   <enabled>true</enabled>
  </model>
  <model>
   <file>Financial_UNIBI/model.xml</file>
   <enabled>true</enabled>
  </model>
 </models>
 <environment>
  <constants>
   <variable>
    <type>int</type>
    <name>print_log</name>
    <description>1, A flag to print logging output to terminal.</description>
   </variable>
   <variable>
    <type>int</type>
    <name>print_debug</name>
    <description>1, A flag to print debugging output to terminal.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>gamma_const</name>
    <description> -2, Strength of logit rule for consumption</description>
   </variable>
   <variable>
    <type>double</type>
    <name>alpha</name>
    <description>0.662, Parameter for production function.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>beta</name>
    <description>0.338, Parameter for production function.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>dividend_rate</name>
    <description>0.4, Dividend rate of consumption goods producers.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>depreciation_rate</name>
    <description>0.01, Capital depreciation rate.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>discount_rate</name>
    <description>0.05, Discount rate.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>teta</name>
    <description>0.0, Strength of adaption of the capital stock if firm is rationed on the labor market.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>mark_up</name>
    <description>0.2, Pricing rule: mark up on unit costs.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>lambda</name>
    <description>0.5, Strength of the influence of the actual demand on the next production quantity: if LAMBDA = 0 then the planned production quantities of the last periods are recognized. If LAMBDA = 1 then only the actual demand is recognized.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>wage_update</name>
    <description>0.02, Parameter for adaption of the wage offer: percentage</description>
   </variable>
   <variable>
    <type>int</type>
    <name>min_vacancy</name>
    <description>2, minimum number of vacancies to trigger vacancy counter</description>
   </variable>
   <variable>
    <type>double</type>
    <name>wage_reservation_update</name>
    <description>0.02, Parameter adaption of the reservation wage: percentage.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>region_cost_1</name>
    <description>0.2, Cost of working in a different region: commuting costs.</description>
   </variable>
<variable>
    <type>double</type>
    <name>region_cost_2</name>
    <description>0.2, Cost of working in a different region: commuting costs.</description>
   </variable>
<variable>
    <type>double</type>
    <name>region_cost_3</name>
    <description>0.2, Cost of working in a different region: commuting costs.</description>
   </variable>
<variable>
    <type>double</type>
    <name>day_change_region_costs_1</name>
    <description>0.2, Cost of working in a different region: commuting costs.</description>
   </variable>
<variable>
    <type>double</type>
    <name>day_change_region_costs_2</name>
    <description>0.2, Cost of working in a different region: commuting costs.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>transition_phase</name>
    <description> </description>
   </variable>
   <variable>
    <type>int</type>
    <name>inventory_costs_per_unit</name>
    <description>2, Administrative cost of storing one unit for one period.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>on_the_job_search_rate</name>
    <description>10.0, Percentage of employees who are searching for a new job.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>initial_consumption_propensity</name>
    <description>0.95, The initial_consumption_propensity times the mean income is the upper limit of the consumption during the first five periods: this smoothes the consumption expenditures because of high dividend payments of the capital goods producer.</description>
   </variable>
   <variable>
    <type>double</type>
    <name>consumption_propensity</name>
    <description>0.95, Percentage of savings allocated to consumption.</description>
   </variable>
   <variable>
    <type>int</type>
    <name>firm_planning_horizon</name>
    <description>10, Planning horizon of firms</description>
   </variable>
   <variable>
    <type>double</type>
    <name>debt_assets_ratio_threshold</name>
    <description>0.8</description>
   </variable>
   <variable>
    <type>double</type>
    <name>interest_rate</name>
    <description>0.005</description>
   </variable>
   <variable>
    <type>double</type>
    <name>inv_inertia</name>
    <description>3</description>
   </variable>
   <variable>
    <type>double</type>
    <name>adaption_delivery_volume</name>
    <description>0.05</description>
   </variable>
   <variable>
    <type>int</type>
    <name>periods_to_repay_loans</name>
    <description>12</description>
   </variable>
   <variable>
    <type>int</type>
    <name>delivery_prob_if_critical_stock_0</name>
    <description>25, Probability for the delivery if the critical stock of one mall was zero for the last periods.</description>
   </variable>
   <variable>
    <type>int</type>
    <name>innovation_probability</name>
    <description>10</description>
   </variable>
   <variable>
    <type>double</type>
    <name>productivity_progress</name>
    <description>0.05</description>
   </variable>
<variable>
    <type>double</type>
    <name>logit_parameter_specific_skills</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>logit_parameter_general_skills</name>
    <description></description>
   </variable>
   <variable>
    <type>int</type>
    <name>lower_bound_firing</name>
    <description></description>
   </variable>
   <variable>
    <type>int</type>
    <name>upper_bound_firing</name>
    <description></description>
   </variable>
   <variable>
    <type>int</type>
    <name>const_bankruptcy_idle_period</name>
    <description></description>
   </variable>
   <variable>
    <type>int</type>
    <name>region_subsidy</name>
    <description>region ID of the region which will receive subsidies</description>
   </variable>
   <variable>
    <type>double</type>
    <name>subsidy_fraction</name>
    <description>fraction of cap_good_price which is subsidized</description>
   </variable>
<variable>
    <type>double</type>
    <name>efficiency_innovation</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>percent_revenue_for_innovation</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>ig_lambda</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>production_productivity</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>ig_mark_up</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>max_capital_good_price_increase</name>
    <description></description>
   </variable>
<variable>
    <type>double</type>
    <name>additional_capital_good_production</name>
    <description></description>
   </variable>
  </constants>
  <functionFiles>
   <file>my_library_functions.c</file>
  </functionFiles>
  <timeUnits>
   <timeUnit>
    <name>daily</name>
    <unit>iteration</unit>
    <period>1</period>
   </timeUnit>
   <timeUnit>
    <name>weekly</name>
    <unit>daily</unit>
    <period>5</period>
   </timeUnit>
   <timeUnit>
    <name>monthly</name>
    <unit>weekly</unit>
    <period>4</period>
   </timeUnit>
   <timeUnit>
    <name>quarterly</name>
    <unit>monthly</unit>
    <period>3</period>
   </timeUnit>
   <timeUnit>
    <name>yearly</name>
    <unit>monthly</unit>
    <period>12</period>
   </timeUnit>
   <timeUnit>
    <name>millionyearly</name>
    <unit>yearly</unit>
    <period>1000000</period>
   </timeUnit>
  </timeUnits>
  <dataTypes/>
 </environment>
