\n  <xagent>\r\n   <name>Clearinghouse</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>region_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>partition_id</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions/>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>bank_account_update</name>\r\n   <description>Sent by household. Household informs the bank about the actual&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\n payment account</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>central_bank_account_update</name>\r\n   <description>Banks or Governments send account information to central bank</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message