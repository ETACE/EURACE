iable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>critical_stock</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>current_stock</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>mall_quality_price_info</name>\r\n    <description>Used by household to hold information about quality and prices of one good offered by mall</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quality</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>price</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>available</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>mall_stock</name>\r\n    <description>Used by mall to hold information about current stock</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>stock</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>price</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quality</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>previous_price</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>delivery_volume_per_mall</name>\r\n    <description>Used by firm to hold information about delivery volume per mall</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>price</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quality</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>logit_firm_id</name>\r\n    <description>Used by household to stock logits and corresponding firm IDs</description>\r\n    <variables>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>logit</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>sales_in_mall</name>\r\n    <description>Used by mall to calculate sales</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\