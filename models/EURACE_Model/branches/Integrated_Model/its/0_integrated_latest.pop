S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Eurace</name>\r\n <version>Aix, 10/02/2009</version>\r\n <description>Authors: Simon Coakley, Mariam Kiran, Simon Gemkow, Philipp Harting, Sander van der Hoog, Mario Locci, Sabrina Ecca, Marco Raberto, Andrea Teglio, Saul Desiderio, Federico Scarpa.</description>\r\n <models>\r\n  <model>\r\n   <file>Cons_Goods_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Credit_Ancona/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Financial_Management_GREQAM/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>FINANCIAL_UG/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Financial_UNIBI/model.xml</file>\r\n   <enabled>false</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Inv_Goods_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Labour_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Government_GREQAM/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Statistical_Office_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n </models>\r\n <environment>\r\n  <constants>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_log</name>\r\n    <description>0 or 1, A flag to print logging output to terminal or to file.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_debug</name>\r\n    <description>0 or 1, A flag to print debugging output to terminal.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>switch_flow_consistency_check</name>\r\n    <description>Switch to turn on flow consistency checking (run daily).</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>switch_stock_consistency_check</name>\r\n    <description>Switch to turn on stock consistency checking (run monthly).</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>number_of_banks_to_apply</name>\r\n    <description>number of banks firms can ask loans</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>gamma_const</name>\r\n    <description> -2, Strength of logit rule for consumption</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>alpha</name>\r\n    <description>0.662, Parameter for production function.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>beta</name>\r\n    <description>0.338, Parameter for production function.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>depreciation_rate</name>\r\n    <description>0.01, Capital depreciation rate.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>discount_rate</name>\r\n    <description>0.05, Discount rate.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>teta</name>\r\n    <description>0.0, Strength of adaption of the capital stock if firm is rationed on the labor market.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>mark_up</name>\r\n    <description>0.2, Pricing rule: mark up on unit costs.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>lambda</name>\r\n    <description>0.5, Strength of the influence of the actual demand on the next production quantity: if LAMBDA = 0 then the planned production quantities of the last periods are recognized. If LAMBDA = 1 then only the actual demand is recognized.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>wage_update</name>\r\n    <description>0.02, Parameter for adaption of the wage offer: percentage</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>min_vacancy</name>\r\n    <description>2, minimum number of vacancies to trigger vacancy counter</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>wage_reservation_update</name>\r\n    <description>0.02, Parameter adaption of the reservation wage: percentage.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>region_cost</name>\r\n    <description>0.2, Cost of working in a different region: commuting costs.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>inventory_costs_per_unit</name>\r\n    <description>2, Administrative cost of storing one unit for one period.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>on_the_job_search_rate</name>\r\n    <description>10.0, Percentage of employees who are searching for a new job.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>initial_consumption_propensity</name>\r\n    <description>0.95, The initial_consumption_propensity times the mean income is the upper limit of the consumption during the first five periods: this smoothes the consumption expenditures because of high dividend payments of the capital goods producer.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>consumption_propensity</name>\r\n    <description>0.95, Percentage of savings allocated to consumption.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>firm_planning_horizon</name>\r\n    <description>10, Planning horizon of firms</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>inv_inertia</name>\r\n    <description>3</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>adaption_delivery_volume</name>\r\n    <description>