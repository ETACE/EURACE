r\n      <type>double</type>\r\n      <name>price</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>assetId</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>Belief</name>\r\n    <description>ADT Belief </description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>asset_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>expectedPriceReturns</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>expectedTotalReturns</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>expectedCashFlowYield</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>volatility</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>expectedEarning</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>expectedEarningPayout</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>last_price</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>utility</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>Stock</name>\r\n    <description>ADT Stock</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>nrOutStandingShares</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>index</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>prices[100]</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>returns[100]</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>Bond</name>\r\n    <description>ADT Bond</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>nr_outstanding</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>face_value</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>nominal_yield</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>maturity_day</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>issue_day</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>yield2maturity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>prices[100]</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>index</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>returns[100]</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>Asset</name>\r\n    <description>ADT Asset owned</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>lastPrice</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>ClearingMechanism</name>\r\n    <description>ADT clearing mechanism </description>\r\n    <variables>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>lastPrice</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>Order_array</type>\r\n      <name>sellOrders</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>Order_array</type>\r\n      <name>buyOrders</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double_array</type>\r\n      <name>prices</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Household</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>risk_free_rate</name>\r\n     <description>Remuneration rate sent by Bank</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wealth</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Belief_array</type>\r\n     <name>beliefs</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order_array</type>\r\n     <name>pendingOrders</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Asset_array</type>\r\n     <name>assetsowned</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetWeights</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetUtilities</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash_on_hand</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>forwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>backwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bins</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>randomWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>fundamentalWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>chartistWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>holdingPeriodToForwardW</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>lossaversion</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>strategy</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Household_receive_info_interest_from_bank</name>\r\n     <description></description>\r\n     <currentState>Household_Start_Financial_Market_Role</currentState>\r\n     <nextState>SELECTSTRATEGY</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>accountInterest</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_select_strategy</name>\r\n     <description></description>\r\n     <currentState>SELECTSTRATEGY</currentState>\r\n     <nextState>CHOOSE_TO_TRADE_OR_NOT</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_stock_beliefs_formation</name>\r\n     <description></description>\r\n     <currentState>CHOOSE_TO_TRADE_OR_NOT</currentState>\r\n     <nextState>BOND_BELIEF_FORMATION</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.strategy</value>\r\n      </lhs>\r\n      <op>NEQ</op>\r\n      <rhs>\r\n       <value>0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>info_firm</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_bond_beliefs_formation</name>\r\n     <description></description>\r\n     <currentState>BOND_BELIEF_FORMATION</currentState>\r\n     <nextState>SEND_ORDERS</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.strategy</value>\r\n      </lhs>\r\n      <op>NEQ</op>\r\n      <rhs>\r\n       <value>0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>info_bond</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_does_not_trading</name>\r\n     <description></description>\r\n     <currentState>CHOOSE_TO_TRADE_OR_NOT</currentState>\r\n     <nextState>Household_Start_Labour_Role</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.strategy</value>\r\n      </lhs>\r\n      <op>EQ</op>\r\n      <rhs>\r\n       <value>0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_send_orders</name>\r\n     <description></description>\r\n     <currentState>SEND_ORDERS</currentState>\r\n     <nextState>WAITORDERSTATUS</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Household_update_its_portfolio</name>\r\n     <description></description>\r\n     <currentState>WAITORDERSTATUS</currentState>\r\n     <nextState>Household_Start_Labour_Role</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order_status</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.trader_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Clearinghouse</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>Asset_array</type>\r\n     <name>assets</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>ClearingMechanism</type>\r\n     <name>clearingmechanism</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>ClearingHouse_receive_info</name>\r\n     <description></description>\r\n     <currentState>start_Clearinghouse</currentState>\r\n     <nextState>RECEIVEDINFOSTOCK</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>info_firm</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>info_bond</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_receive_orders_and_run</name>\r\n     <description></description>\r\n     <currentState>RECEIVEDINFOSTOCK</currentState>\r\n     <nextState>COMPUTEDPRICES</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order_status</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_send_asset_information</name>\r\n     <description></description>\r\n     <currentState>COMPUTEDPRICES</currentState>\r\n     <nextState>end_Clearinghouse</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>infoAssetCH</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory/>\r\n   <functions>\r\n    <function>\r\n     <name>Bank_send_accountInterest</name>\r\n     <description></description>\r\n     <currentState>start_Bank</currentState>\r\n     <nextState>Bank_start_credit_market_role</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>accountInterest</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings_payout</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>current_shares_outstanding</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Stock</type>\r\n     <name>stock</