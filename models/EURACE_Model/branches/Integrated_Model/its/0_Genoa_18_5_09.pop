tState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_request</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_conditions</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_give_loan</name>\r\n     <description></description>\r\n     <currentState>Bank_03</currentState>\r\n     <nextState>Bank_04</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_acceptance</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_receive_installment</name>\r\n     <description></description>\r\n     <currentState>Bank_04</currentState>\r\n     <nextState>Bank_05</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>installment</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n      <input>\r\n       <messageName>bankruptcy</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_account_update_deposits</name>\r\n     <description></description>\r\n     <currentState>Bank_05</currentState>\r\n     <nextState>Bank_06</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>bank_account_update</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>central_bank_account_update</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_accounting</name>\r\n     <description></description>\r\n     <currentState>Bank_06</currentState>\r\n     <nextState>Bank_07</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>a.day_of_month_to_act</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_interest_payment</messageName>\r\n      </output>\r\n      <output>\r\n       <messageName>tax_payment</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_idle</name>\r\n     <description></description>\r\n     <currentState>Bank_06</currentState>\r\n     <nextState>end_Bank_cycle</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>a.day_of_month_to_act</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_send_data_to_Eurostat</name>\r\n     <description></description>\r\n     <currentState>Bank_07</currentState>\r\n     <nextState>end_Bank_cycle</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>SWITCH_STOCK_CONSISTENCY_CHECK</value>\r\n       </lhs>\r\n      