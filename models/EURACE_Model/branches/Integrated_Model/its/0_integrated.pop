ts/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>policy_announcement</name>\r\n   <description>Government announcing tax rates, benefits, transfer payments and subsidies.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>gov_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_corporate</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_hh_labour</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_hh_capital</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_vat</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>unemployment_benefit_pct</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>hh_subsidy_payment</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_subsidy_payment</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>hh_transfer_payment</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_transfer_payment</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>tax_payment</name>\r\n   <description>Added by agents to pay taxes</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>gov_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n   