m_End_Labour_Role</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_send_random_redundancies</name>\r\n     <description>Firms fire workers randomly.</description>\r\n     <currentState>03ccc</currentState>\r\n     <nextState>04ccc</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>firing</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description></description>\r\n     <currentState>04ccc</currentState>\r\n     <nextState>03c</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>EQ</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Household</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage</name>\r\n     <description>The wage a household receives</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_reservation</name>\r\n     <description>Reservation wage</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>general_skill</name>\r\n     <description>The general skill level</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>on_the_job_search</name>\r\n     <description>Indicator variable: 1 on the job search; 0 no on the job search</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>number_applications</name>\r\n     <description>Number of applications a household can sent</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>last_labour_income</name>\r\n     <description>The last labour income earned by the house