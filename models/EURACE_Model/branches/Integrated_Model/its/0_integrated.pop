S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Labour_Market</name>\r\n <version>pre-Sheffield, merged UNIBI model, 02/07/2008</version>\r\n <description>Authors: Philipp Harting, Simon Gemkow</description>\r\n <models/>\r\n <environment>\r\n  <constants/>\r\n  <functionFiles>\r\n   <file>Firm_Employer_Functions.c</file>\r\n   <file>Household_Employee_Functions.c</file>\r\n   <file>Labour_aux_functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits/>\r\n  <dataTypes>\r\n   <dataType>\r\n    <name>employee</name>\r\n    <description>Used to hold employee information in firms</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>wage</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>general_skill</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>specific_skill</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>vacancy</name>\r\n    <description>Used by households to hold vacancy information</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>wage_offer</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>jo