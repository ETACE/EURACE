ription>\r\n    </variable>\r\n    <variable>\r\n     <type>Belief_array</type>\r\n     <name>beliefs</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order_array</type>\r\n     <name>pendingOrders</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Asset_array</type>\r\n     <name>assetsowned</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetWeights</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetUtilities</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash_on_hand</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>forwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>backwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bins</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>randomWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>fundamentalWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>chartistWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>holdingPeriodToForwardW</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>lossaversion</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <n