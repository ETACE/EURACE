\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_monthly_resetting</name>\r\n     <description>Monthly resetting of counters.</description>\r\n     <currentState>Government_Start_Monthly_Loop_Top</currentState>\r\n     <nextState>Government_Start_Yearly_Loop_Top</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description>Idle for monthly resetting loop at top.</description>\r\n     <currentState>Government_Start_Monthly_Loop_Top</currentState>\r\n     <nextState>Government_Start_Yearly_Loop_Top</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description>Idle for yearly loop at top.</description>\r\n     <currentState>Government_Start_Yearly_Loop_Top</currentState>\r\n     <nextState>Gov_Start_Bond_Market</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>yearly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_send_policy_announcements</name>\r\n     <description>Function to send the yearly policy announcement message.</description>\r\n     <currentState>00</currentState>\r\n     <nextState>Gov_Start_Bond_Market</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>policy_announcement</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_read_tax_payments</name>\r\n     <description>Function to read the tax revenues and store 