scription>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>const_firm_leverage</name>\r\n    <description>Initial leverage (debt/equity ratio) of each firm.</description>\r\n   </variable>\r\n  </constants>\r\n  <functionFiles>\r\n   <file>my_library_functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits>\r\n   <timeUnit>\r\n    <name>daily</name>\r\n    <unit>iteration</unit>\r\n    <period>1</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>weekly</name>\r\n    <unit>daily</unit>\r\n    <period>5</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>monthly</name>\r\n    <unit>weekly</unit>\r\n    <period>4</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>quarterly</name>\r\n    <unit>monthly</unit>\r\n    <period>3</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>yearly</name>\r\n    <unit>monthly</unit>\r\n    <period>12</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>millionyearly</name>\r\n    <unit>yearly</unit>\r\n    <period>1000000</period>\r\n   </timeUnit>\r\n  </timeUnits>\r\n  <dataTypes/>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>region_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>gov_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>day_of_month_to_act</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>last_day_of_month_to_act</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description></description>\r\n    </variable>\