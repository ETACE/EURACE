S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Labour_Market</name>\r\n <version>pre-Sheffield, merged UNIBI model, 02/07/2008, Aix, 10/02/2009</version>\r\n <description>Authors: Philipp Harting, Simon Gemkow</description>\r\n <models/>\r\n <environment>\r\n  <constants>\r\n     <variable>\r\n    <type>double</type>\r\n    <name>gamma_const</name>\r\n    <description> -2, Strength of logit rule for consumption</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>alpha</name>\r\n    <description>0.662, Parameter for production function.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>beta</name>\r\n    <description>0.338, Parameter for production function.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>depreciation_rate</name>\r\n    <description>0.01, Capital depreciation rate.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>discount_rate</name>\r\n    <description>0.05, Discount rate.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>teta</name>\r\n    <description>0.0, Strength of adaption of the capital stock if firm is rationed on the labor market.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>mark_up</name>\r\n    <description>0.2, Pricing rule: mark up on unit costs.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>lambda</name>\r\n    <description>0.5, Strength of the influence of the actual demand on the next production quantity: if LAMBDA = 0 then the planned production quantities of the last periods are recognized. If LAMBDA = 1 then only the actual demand is recognized.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>wage_update</name>\r\n    <description>0.02, Parameter for adaption of the wage offer: percentage</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>min_vacancy</name>\r\n    <description>2, minimum number of vacancies to trigger vacancy counter</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>wage_reservation_update</name>\r\n    <description>0.02, Parameter adaption of the reservation wage: percentage.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>region_cost</name>\r\n    <description>0.2, Cost of working in a different region: commuting costs.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>inventory_costs_per_unit</name>\r\n    <description>2, Administrative cost of storing one unit for one period.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>on_the_job_search_rate</name>\r\n    <description>10.0, Percentage of employees who are searching for a new job.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>consumption_propensity</name>\r\n    <description>0.95, Percentage of savings allocated to consumption.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>firm_planning_horizon</name>\r\n    <description>10, Planning horizon of firms</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>inv_inertia</name>\r\n    <description>3</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>adaption_delivery_volume</name>\r\n    <description>0.05</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>delivery_prob_if_critical_stock_0</name>\r\n    <description>25, Probability for the delivery if the critical stock of one mall was zero for the last periods.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>innovation_probability</name>\r\n    <description>10</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>productivity_progress</name>\r\n    <description>0.05</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>lower_bound_firing</name>\r\n    <description></description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>upper_bound_firing</name>\r\n    <description></description>\r\n   </variable>\r\n  </constants>\r\n   <functionFiles>\r\n   <file>Firm_Employer_Functions.c</file>\r\n   <file>Household_Employee_Functions.c</file>\r\n   <file>Labour_aux_functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits/>\r\n  <dataTypes>\r\n   <dataType>\r\n    <name>employee</name>\r\n    <description>Used to hold employee information in firms</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>wage</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>general_skill</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>specific_skill</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>vacancy</name>\r\n    <description>Used by households to hold vacancy information</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>wage_offer</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>job_application</name>\r\n    <description>Used by firms to hold job applications</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>worker_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>general_skill</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>specific_skill</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>job_offer</name>\r\n    <description>Used by households and firms to hold job offers</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>wage_offer</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>logit</name>\r\n    <description>Used by firms to stock logits and corresponding worker IDs</description>\r\n    <variables>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>logit_value</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>worker_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>general_skill</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>employee_array</type>\r\n     <name>employees</name>\r\n     <description>Used to store the data-type Employee</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_offer</name>\r\n     <description>The basic wage offer</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>technology</name>\r\n     <description>average technology/productivity of the capital</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>no_employees</name>\r\n     <description>number of employees</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>no_employees_skill_1</name>\r\n     <description>number of employees with general skills 1</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>no_employees_skill_2</name>\r\n     <description>number of employees with general skills 2</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>no_employees_skill_3</name>\r\n     <description>number of employees with general skills 3</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>no_employees_skill_4</name>\r\n     <description>number of employees with general skills 4</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>no_employees_skill_5</name>\r\n     <description>number of employees with general skills 5</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>vacancies</name>\r\n     <description>number of vacancies</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>average_g_skill</name>\r\n     <description>average general skills of the workforce</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>average_s_skill_of_1</name>\r\n     <description>average specific skills of employees with general skills 1</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>average_s_skill_of_2</name>\r\n     <description>average specific skills of employees with general skills 2</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>average_s_skill_of_3</name>\r\n     <description>average specific skills of employees with general skills 3</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>average_s_skill_of_4</name>\r\n     <description>average specific skills of employees with general skills 4</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>average_s_skill_of_5</name>\r\n     <description>average specific skills of employees with general skills 5</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_offer_for_skill_1</name>\r\n     <description>wage offer for workers with skill level 1</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_offer_for_skill_2</name>\r\n     <description>wage offer for workers with skill level 2</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_offer_for_skill_3</name>\r\n     <description>wage offer for workers with skill level 3</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_offer_for_skill_4</name>\r\n     <description>wage offer for workers with skill level 4</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wage_offer_for_skill_5</name>\r\n     <description>wage offer for workers with skill level 5</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>employees_needed</name>\r\n     <description>number of workers a firm needs for next production</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Firm_calculate_specific_skills_and_wage_offer</name>\r\n     <description></description>\r\n     <currentState>Firm_Start_Labour_Role</currentState>\r\n     <nextState>011</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_send_vacancies</name>\r\n     <description>If additional workers are needed the firm sends vacancies messages&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\n espacially the different wage offers for the different general skill groups</description>\r\n     <currentState>04ccc</currentState>\r\n     <nextState>03</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>LT</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>vacancies</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_send_redundancies</name>\r\n     <description>If the firm wants to decrease the workforce it sends redundancies</description>\r\n     <currentState>011</currentState>\r\n     <nextState>03ccc</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>GT</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>firing</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_idle</name>\r\n     <description>Firm does nothing</description>\r\n     <currentState>011</currentState>\r\n     <nextState>03ccc</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>LEQ</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_read_job_applications_send_job_offer_or_rejection</name>\r\n     <description>Firm reads the application, ranks the applicants according to &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\ntheir general and specific skills and sends as many job offers to the &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nfirst ranked applicants as the firm has vacancies to fill. &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nThe other applicants are refused.</description>\r\n     <currentState>03</currentState>\r\n     <nextState>04</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>job_application</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.firm_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>job_offer</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_read_job_responses</name>\r\n     <description>The firm reads the responses to their job offers and updates the number of &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nemployees and the number of vacancies</description>\r\n     <currentState>04</currentState>\r\n     <nextState>05a</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>job_acceptance</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.firm_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_read_job_quitting</name>\r\n     <description>The firm reads quitting messages and updates the number &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nof employees and the number of vacancies</description>\r\n     <currentState>05a</currentState>\r\n     <nextState>05b</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>quitting</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_read_job_quitting</name>\r\n     <description>The firm reads quitting messages and updates the number &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nof employees and the number of vacancies</description>\r\n     <currentState>00b</currentState>\r\n     <nextState>09c</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>quitting</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_read_job_quitting</name>\r\n     <description>The firm reads quitting messages and updates the number &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nof employees and the number of vacancies</description>\r\n     <currentState>03c</currentState>\r\n     <nextState>03d</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>quitting</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_start_labour_market</name>\r\n     <description>Dummy function if a firm has to enter the Labor Market &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nin the second round after receiving quittings</description>\r\n     <currentState>03d</currentState>\r\n     <nextState>06</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>LT</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_finish_labour_market_first_round</name>\r\n     <description></description>\r\n     <currentState>03d</currentState>\r\n     <nextState>09a</nextState>\r\n     <condition>\r\n      <not>\r\n       <lhs>\r\n        <value>a.no_employees</value>\r\n       </lhs>\r\n       <op>LT</op>\r\n       <rhs>\r\n        <value>a.employees_needed</value>\r\n       </rhs>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_finish_labour_market_first_round</name>\r\n     <description></description>\r\n     <currentState>05b</currentState>\r\n     <nextState>09a</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>EQ</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_update_wage_offer</name>\r\n     <description>The firm increases the wage offer if there are vacancies left</description>\r\n     <currentState>05b</currentState>\r\n     <nextState>06</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.no_employees</value>\r\n      </lhs>\r\n      <op>LT</op>\r\n      <rhs>\r\n       <value>a.employees_needed</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Firm_send_vacancies_2</name>\r\n     <description>If additional workers are needed the firm sends vacancies messages&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\n espacially the different wage offers for the different general skill groups</description>\r\n     <currentState>06</currentState>\r\n     <nextState>07</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>vacancies2</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_read_job_applications_send_job_offer_or_rejection_2</name>\r\n     <description>Firm reads the application, ranks the applicants according to &#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#x