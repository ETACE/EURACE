n      <type>double</type>\r\n      <name>sales</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>ordered_quantity</name>\r\n    <description>Used by household to stock ordered quantities</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>price</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>received_quantities</name>\r\n    <description>Used by household to stock received quantities</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>sold_quantities_per_mall</name>\r\n    <description>Used by firm to hold informations about sales </description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>mall_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>sold_quantity</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>stock_empty</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>estimated_demand</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>data_type_sa