S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Eurace</name>\r\n <version>Aix, 10/02/2009</version>\r\n <description>Authors: Simon Coakley, Mariam Kiran, Simon Gemkow, Philipp Harting, Sander van der Hoog, Mario Locci, Sabrina Ecca, Marco Raberto, Andrea Teglio, Saul Desiderio, Federico Scarpa.</description>\r\n <models>\r\n  <model>\r\n   <file>Cons_Goods_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Credit_Ancona/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Financial_Management_GREQAM/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>FINANCIAL_UG/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Financial_UNIBI/model.xml</file>\r\n   <enabled>false</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Inv_Goods_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Labour_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Government_GREQAM/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n  <model>\r\n   <file>Statistical_Office_UNIBI/model.xml</file>\r\n   <enabled>true</enabled>\r\n  </model>\r\n </models>\r\n <environment>\r\n  <constants>\r\n    <variable>\r\n    <type>int</type>\r\n    <name>total_regions</name>\r\n    <description>Total number of regions.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_log</name>\r\n    <description>0 or 1, A flag to print logging output to terminal or to file.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_debug_afm</name>\r\n    <description>0 or 1, A flag to print logging output to terminal or to file.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_debug_gov</name>\r\n    <description>0 or 1, A flag to print logging output to terminal or to file.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_debug_consumption</name>\r\n    <description>0 or 1, A flag to print logging output to terminal or to file.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>print_debug</name>\r\n    <description>0 or 1, A flag to print debugging output to terminal.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>switch_flow_consistency_check</name>\r\n    <description>Switch to turn on flow consistency checking (run daily).</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>switch_stock_consistency_check</name>\r\n    <description>Switch to turn on stock consistency checking (run monthly).</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>const_bankruptcy_idle_period</name>\r\n    <description>Number of iterations that a bankrupt firm remains idle, before resuming production activity.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>days_per_month</name>\r\n    <description>Optional setting for the number of days in a month.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>number_of_banks_to_apply</name>\r\n    <description>number of banks firms can ask loans</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>const_number_of_banks</name>\r\n    <description>Total number of banks.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>int</type>\r\n    <name>const_installment_periods</name>\r\n    <description></description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>const_wage_wealth_ratio</name>\r\n    <description>The household\'s initial ratio between wage and wealth. This parametrises the link between the unit price of capital and the unit price of labour.</description>\r\n   </variable>\r\n   <variable>\r\n    <type>double</type>\r\n    <name>const_firm_leverage</name>\r\n    <description>Initial leverage (debt/equity ratio) of each firm.</description>\r\n   </variable>\r\n  </constants>\r\n  <functionFiles>\r\n   <file>my_library_functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits>\r\n   <timeUnit>\r\n    <name>daily</name>\r\n    <unit>iteration</unit>\r\n    <period>1</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>weekly</name>\r\n    <unit>daily</unit>\r\n    <period>5</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>monthly</name>\r\n    <unit>weekly</unit>\r\n    <period>4</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>quarterly</name>\r\n    <unit>monthly</unit>\r\n    <period>3</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>yearly</name>\r\n    <unit>monthly</unit>\r\n    <period>12</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>millionyearly</name>\r\n    <unit>yearly</unit>\r\n    <period>1000000</period>\r\n   </timeUnit>\r\n  </timeUnits>\r\n  <dataTypes/>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>region_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>gov_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>day_of_month_to_act</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>last_day_of_month_to_act</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description></description>\r\n 