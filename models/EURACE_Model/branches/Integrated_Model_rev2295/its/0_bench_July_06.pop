S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Credit_Market</name>\r\n <version>Aix, 10/02/2009</version>\r\n <description></description>\r\n <models/>\r\n <environment>\r\n  <constants/>\r\n  <functionFiles>\r\n   <file>Bank_Credit_Functions.c</file>\r\n   <file>Firm_Credit_Functions.c</file>\r\n   <file>Central_Bank_Functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits>\r\n   <timeUnit>\r\n    <name>daily</name>\r\n    <unit>iteration</unit>\r\n    <period>1</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>weekly</name>\r\n    <unit>daily</unit>\r\n    <period>5</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>monthly</name>\r\n    <unit>weekly</unit>\r\n    <period>4</period>\r\n   </timeUnit>\r\n  </timeUnits>\r\n  <dataTypes>\r\n   <dataType>\r\n    <name>potential_lender</name>\r\n    <description>Collecing the name of an active bank found by firm when entering its credit market role</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>bank_name</name>\r\n      <description>Name of Bank</description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>contacted</name>\r\n      <description>if the bank has been contacted (1) or not (0)</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>account_item</name>\r\n    <description></description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description>id of the account\'s holder</description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>payment_account</name>\r\n      <description>value in the account</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <ty