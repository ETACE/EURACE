  <variable>\r\n      <type>double_array</type>\r\n      <name>prices</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Household</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>risk_free_rate</name>\r\n     <description>Remuneration rate sent by Bank</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>wealth</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Belief_array</type>\r\n     <name>beliefs</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order_array</type>\r\n     <name>pendingOrders</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Asset_array</type>\r\n     <name>assetsowned</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetWeights</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetUtilities</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash_on_hand</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>forwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>backwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bins</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>randomWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <ty