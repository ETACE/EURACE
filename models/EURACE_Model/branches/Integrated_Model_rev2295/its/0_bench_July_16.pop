ns>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>productivity</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>IGfirm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cap_productivity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cap_good_price</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>capital_good_request</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_demand</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>capital_good_profit_distribution</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_profit_shares</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>capital_good_delivery</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_delivery_volume</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>productivity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_price</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\