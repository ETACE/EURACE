     <name>bond</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order</type>\r\n     <name>pending_order</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Government_pays_coupons</name>\r\n     <description></description>\r\n     <currentState>Gov_Start_Bond_Market</currentState>\r\n     <nextState>Gov_rate_upgrade</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_read_policy_rate</name>\r\n     <description>Government reads the policy rate from the CB and updates the bonds yield</description>\r\n     <currentState>Gov_rate_upgrade</currentState>\r\n     <nextState>GOVERNMENT_paid_coupons</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>policy_rate</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_send_info_bond</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_paid_coupons</currentState>\r\n     <nextState>GOVERNMENT_SENT_info_bond</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>info_bond</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_orders_issuing</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_SENT_info_bond</currentState>\r\n     <nextState>GOVERNMENT_SENT_ORDER</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_update_its_portfolio</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_SENT_ORDER</currentState>\r\n     <nextState>GOVERNMENT_PORTFOLIO_UPDATED</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order_status</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.trader_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_receive_info_bond</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_PORTFOLIO_UPDATED</currentState>\r\n     <nextState>01</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>infoAssetCH</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.asset_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description></description>\r\n     <currentState>Gov_Start_Bond_Market</currentState>\r\n     <nextState>GOVERNMENT_paid_coupons</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>order</name>\r\n   <description>order posted by the Household</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>trader_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>limit_price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>order_status</name>\r\n   <description>order status posted by clearing house</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>trader_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>infoAssetCH</name>\r\n   <description>message posted by the Clearing House </description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>info_firm</name>\r\n   <description>message posted by the Firm</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings_payout</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Stock</type>\r\n     <name>stock</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>info_bond</name>\r\n   <description>message posted by the Government </description>\r\n   <variables>\r\n    <variable>\r\n     <type>Bond</type>\r\n     <name>bond</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>payment_coupons</name>\r\n   <description>message posted by the Government, announce the payment coupons and it decreases its payment_account </description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>coupon</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>government_id</name>\r\n     <description>the unitary cost of a coupon</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n </messages>\r\n</xmodel>\r\n'
