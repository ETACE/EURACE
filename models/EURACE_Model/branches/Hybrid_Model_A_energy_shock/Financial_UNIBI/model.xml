     </variable>
     <variable>
      <type>double</type>
      <name>expectedEarningPayout</name>
      <description></description>
     </variable>
     <variable>
      <type>double</type>
      <name>last_price</name>
      <description></description>
     </variable>
     <variable>
      <type>double</type>
      <name>utility</name>
      <description></description>
     </variable>
    </variables>
   </dataType>
   <dataType>
    <name>Stock</name>
    <description>ADT Stock</description>
    <variables>
     <variable>
      <type>int</type>
      <name>id</name>
      <description></description>
     </variable>
