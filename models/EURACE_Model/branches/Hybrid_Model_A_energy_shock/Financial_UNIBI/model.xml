     <variable>
      <type>int</type>
      <name>quantity</name>
      <description></description>
     </variable>
     <variable>
      <type>Order_array</type>
      <name>sellOrders</name>
      <description></description>
     </variable>
     <variable>
      <type>Order_array</type>
      <name>buyOrders</name>
      <description></description>
     </variable>
     <variable>
      <type>double_array</type>
      <name>prices</name>
      <description></description>
