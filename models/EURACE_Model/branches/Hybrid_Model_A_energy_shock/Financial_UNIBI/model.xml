      <type>int</type>
      <name>assetId</name>
      <description></description>
     </variable>
    </variables>
   </dataType>
   <dataType>
    <name>Belief</name>
    <description>ADT Belief </description>
    <variables>
     <variable>
      <type>int</type>
      <name>asset_id</name>
      <description></description>
     </variable>
     <variable>
      <type>double</type>
      <name>expectedPriceReturns</name>
      <description></description>
     </variable>
     <variable>
      <type>double</type>
