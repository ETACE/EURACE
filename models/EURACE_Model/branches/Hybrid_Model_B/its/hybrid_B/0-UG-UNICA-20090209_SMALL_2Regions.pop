S'<?xml version="1.0" encoding="ISO-8859-1"?>\r\n<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2">\r\n\r\n<name>Credit_Market</name>\r\n<version>Genoa 09/05/2008</version>\r\n<description></description>\r\n\r\n\r\n<environment>\r\n\r\n\r\n<functionFiles>\r\n<file>Bank_Credit_Functions.c</file>\r\n<file>Firm_Credit_Functions.c</file>\r\n</functionFiles>\r\n\r\n\r\n<timeUnits>\r\n\r\n<timeUnit>\r\n <name>daily</name>\r\n <unit>iteration</unit>\r\n <period>1</period>\r\n</timeUnit>\r\n\r\n<timeUnit>\r\n <name>weekly</name>\r\n <unit>daily</unit>\r\n <period>5</period>\r\n</timeUnit>\r\n\r\n<timeUnit>\r\n <name>monthly</name>\r\n <unit>weekly</unit>\r\n <period>4</period>\r\n</timeUnit>\r\n</timeUnits>\r\n\r\n<dataTypes>\r\n<dataType>\r\n<name>residual_var_datatype</name>\r\n<description></description>\r\n<variables>\r\n<variable><type>double</type><name>value[10]</name><description>Bank providing the loan</description></variable>\r\n</variables>\r\n</dataType>\r\n</dataTypes>\r\n\r\n</environment>\r\n\r\n<agents>\r\n<xagent>\r\n<name>Firm</name>\r\n<description></description>\r\n<memory>\r\n<variable><type>int</type><name>dmarketmatrix[10]</name><description></description></variable>\r\n<variable><type>residual_var_datatype</type><name>residual_var[10]</name><description></description></variable>\r\n</memory>\r\n\r\n<functions>\r\n\r\n<function>\r\n<name>Firm_ask_loan</name>\r\n<description></description>\r\n<currentState>Firm_Start_Credit_Role</currentState>\r\n<nextState>Firm_Credit_02</nextState>\r\n<condition>\r\n<lhs><lhs><value>a.external_financial_needs</value></lhs><op>GT</op><rhs><value>0.0</value></rhs></lhs>\r\n <op>AND</op>\r\n<rhs><lhs><value>a.credit_dummy</value></lhs><op>EQ</op><rhs><value>0</value></rhs></rhs>\r\n</condition>\r\n<outputs>\r\n<output><messageName>loan_request</messageName></output>\r\n</outputs>\r\n</function>\r\n\r\n<function>\r\n<name>Firm_get_loan</name>\r\n<description></description>\r\n<currentState>Firm_Credit_02</currentState>\r\n<nextState>Firm_End_Credit_Role</nextState>\r\n\r\n<inputs>\r\n<input>\r\n<messageName>loan_conditions</messageName>\r\n<filter>\r\n  <lhs><value>a.id</value></lhs>\r\n  <op>EQ</op>\r\n  <rhs><value>m.firm_id</value></rhs>\r\n </filter>\r\n</input>\r\n</inputs>\r\n<outputs>\r\n<output><messageName>loan_acceptance</messageName></output>\r\n</outputs>\r\n</function>\r\n\r\n</functions>\r\n</xagent>\r\n\r\n\r\n\r\n<xagent>\r\n<name>Bank</name>\r\n<description></description>\r\n<memory>\r\n<variable><type>double</type><name>cash</name><description></description></variable>\r\n<variable><type>double</type><name>total_credit</name><description></description></variable>\r\n<variable><type>double</type><name>equity</name><description></description></variable>\r\n<variable><type>double</type><name>bce_debt</name><description></description></variable>\r\n<variable><type>double</type><name>alfa</name><description></description></variable>\r\n<variable><type>double</type><name>value_at_risk</name><description></description></variable>\r\n<variable><type>double</type><name>min_interest</name><description></description></variable>\r\n<variable><type>double</type><name>bank_gamma[2]</name><description></description></variable>\r\n<variable><type>double</type><name>profits[2]</name><description></description></variable>\r\n<variable><type>double</type><name>bank_lambda</name><description></description></variable>\r\n<variable><type>double</type><name>bce_interest</name><description></description></variable>\r\n<variable><type>double</type><name>bank_dividend_rate</name><description></description></variable>\r\n<variable><type>double</type><name>tax_rate_corporate</name><description></description></variable>\r\n<variable><type>int</type><name>number_of_shares</name><description></description></variable>\r\n<variable><type>double</type><name>debt_period</name><description></description></variable>\r\n<variable><type>int</type><name>loan_request_message_found</name><description></description></variable>\r\n<variable><type>int</type><name>day_of_month_to_act</name><description>Day of the month to act for bank accounting function</description></variable>\r\n</memory>\r\n\r\n\r\n<functions>\r\n<!--\r\n<function><name>Bank_idle</name>\r\n<description></description>\r\n<currentState>start_Bank</currentState>\r\n<nextState>Bank_01</nextState>\r\n</function>\r\n-->\r\n<function>\r\n<name>Bank_decide_credit_conditions</name>\r\n<description></description>\r\n<currentState>Bank_start_credit_market_role</currentState>\r\n<nextState>Bank_02</nextState>\r\n<inputs>\r\n<input><messageName>loan_request</messageName>\r\n<filter>\r\n  <lhs><value>a.id</value></lhs>\r\n  <op>EQ</op>\r\n  <rhs><value>m.bank_id</value></rhs>\r\n </filter>\r\n</input>\r\n</inputs>\r\n<outputs>\r\n<output><messageName>loan_conditions</messageName></output>\r\n</outputs>\r\n</function>\r\n\r\n\r\n\r\n<function>\r\n<name>Bank_give_loan</name>\r\n<description></description>\r\n<currentState>Bank_02</currentState>\r\n<nextState>Bank_03</nextState>\r\n<inputs>\r\n<input>\r\n<messageName>loan_acceptance</messageName>\r\n<filter>\r\n  <lhs><value>a.id</value></lhs>\r\n  <op>EQ</op>\r\n  <rhs><value>m.bank_id</value></rhs>\r\n</filter>\r\n</input>\r\n</inputs>\r\n</function>\r\n\r\n\r\n\r\n<function>\r\n<name>Bank_receive_installment</name>\r\n<description></description>\r\n<currentState>Bank_03</currentState>\r\n<nextState>Bank_04</nextState>\r\n<inputs>\r\n<input>\r\n<messageName>installment</messageName>\r\n<filter>\r\n  <lhs><value>a.id</value></lhs>\r\n  <op>EQ</op>\r\n  <rhs><value>m.bank_id</value></rhs>\r\n</filter>\r\n</input>\r\n<input>\r\n<messageName>bankruptcy</messageName>\r\n<filter>\r\n  <lhs><value>a.id</value></lhs>\r\n  <op>EQ</op>\r\n  <rhs><value>m.bank_id</value></rhs>\r\n</filter>\r\n</input>\r\n</inputs>\r\n</function>\r\n\r\n\r\n\r\n<function>\r\n<name>Bank_account_update_deposits</name>\r\n<description></description>\r\n<currentState>Bank_04</currentState>\r\n<nextState>Bank_05</nextState>\r\n<inputs>\r\n<input>\r\n<messageName>bank_account_update</messageName>\r\n<filter>\r\n  <lhs><value>a.id</value></lhs>\r\n  <op>EQ</op>\r\n  <rhs><value>m.bank_id</value></rhs>\r\n</filter>\r\n</input>\r\n</inputs>\r\n<!--<outputs>\r\n<output><messageName>central_bank_account_update</messageName></output>\r\n</outputs>-->\r\n</function>\r\n\r\n\r\n\r\n<function>\r\n<name>Bank_accounting</name>\r\n<description></description>\r\n<currentState>Bank_05</currentState>\r\n<nextState>end_Bank_cycle</nextState>\r\n<condition>\r\n    <time>\r\n    <period>monthly</period>\r\n    <phase>a.day_of_month_to_act</phase>\r\n    </time>\r\n</condition>\r\n</function>\r\n\r\n\r\n\r\n<function>\r\n<name>Bank_idle</name>\r\n<description></description>\r\n<currentState>Bank_05</currentState>\r\n<nextState>end_Bank_cycle</nextState>\r\n<condition>\r\n\t<not>\r\n    <time>\r\n    <period>monthly</period>\r\n    <phase>a.day_of_month_to_act</phase>\r\n    </time>\r\n    </not>\r\n</condition>\r\n</function>\r\n\r\n\r\n</functions>\r\n\r\n</xagent>\r\n\r\n</agents>\r\n\r\n\r\n\r\n<messages>\r\n\r\n<message>\r\n<name>loan_request</name>\r\n<description>Message added by firm to demand credit with bank_id, with financial info of applying firm.</description>\r\n<variables>\r\n<variable><type>int</type><name>firm_id</name><description></description></variable>\r\n<variable><type>int</type><name>bank_id</name><description></description></variable>\r\n<variable><type>double</type><name>equity</name><description></description></variable>\r\n<variable><type>double</type><name>total_debt</name><description></description></variable>\r\n<variable><type>double</type><name>external_financial_needs</name><description></description></variable>\r\n</variables>\r\n</message>\r\n\r\n\r\n<message>\r\n<name>loan_conditions</name>\r\n<description> Message added by bank to offer credit; contains the interest rate, the amount of offered credit, and the value_at_risk.</description>\r\n<variables>\r\n<variable><type>int</type><name>firm_id</name><description></description></variable>\r\n<variable><type>int</type><name>bank_id</name><description></description></variable>\r\n<variable><type>double</type><name>proposed_interest_rate</name><description></description></variable>\r\n<variable><type>double</type><name>amount_offered_credit</name><description></description></variable>\r\n<variable><type>double</type><name>value_at_risk</name><description></description></variable>\r\n</variables>\r\n</message>\r\n\r\n\r\n<message>\r\n<name>loan_acceptance</name>\r\n<description>Message added by firm to accept a loan with bank_id, for the amount credit_taken and VAR. The bank does not need to know the firm_id.</description>\r\n<variables>\r\n<variable><type>int</type><name>bank_id</name><description></description></variable>\r\n<variable><type>double</type><name>credit_amount_taken</name><description></description></variable>\r\n<variable><type>double</type><name>loan_total_var</name><description></description></variable>\r\n</variables>\r\n</message>\r\n\r\n<message>\r\n<name>installment</name>\r\n<description>Message added by firm pays installment and interest to the bank.</description>\r\n<variables>\r\n<variable><type>int</type><name>bank_id</name><description></description></variable>\r\n<variable><type>double</type><name>installment_amount</name><description>installment_amount is 0 in case of bankruptcy, positive in case of No_bankruptcy</description></variable>\r\n<variable><type>double</type><name>interest_amount</name><description></description></variable>\r\n<variable><type>double</type><name>var_per_installment</name><description></description></variable>\r\n</variables>\r\n</message>\r\n\r\n<message>\r\n<name>bankruptcy</name>\r\n<description>Message added by firm to bank to signal bankruptcy.</description>\r\n<variables>\r\n<variable><type>int</type><name>bank_id</name><description></description></variable>\r\n<variable><type>double</type><name>bad_debt</name><description></description></variable>\r\n<variable><type>double</type><name>credit_refunded</name><description></description></variable>\r\n<variable><type>double</type><name>residual_var</name><description></description></variable>\r\n</variables>\r\n</message>\r\n\r\n<!--\r\n<message>\r\n<name>central_bank_account_update</name>\r\n<description>message for central bankto new name</description>\r\n<variables>\r\n<variable><type>double</type><name>payment_account</name><description></description></variable>\r\n<variable><type>int</type><name>bank_id</name><description></description></variable>\r\n</variables>\r\n</message>\r\n-->\r\n\r\n<message>\r\n<name>BCE_return</name>\r\n<description></description>\r\n<variables>\r\n<variable><type>int</type><name>bce_debt</name><description></description></variable>\r\n<variable><type>int</type><name>id</name><description></description></variable>\r\n</variables>\r\n</message>\r\n\r\n</messages>\r\n\r\n</xmodel>\r\n\r\n'
