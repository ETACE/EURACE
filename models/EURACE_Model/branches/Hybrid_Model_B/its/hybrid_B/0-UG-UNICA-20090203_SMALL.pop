n    <function>\r\n     <name>ClearingHouse_receive_orders_and_run</name>\r\n     <description></description>\r\n     <currentState>RECEIVEDINFOSTOCK</currentState>\r\n     <nextState>COMPUTEDPRICES</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order_status</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_send_asset_information</name>\r\n     <description></description>\r\n     <currentState>COMPUTEDPRICES</currentState>\r\n     <nextState>end_Clearinghouse</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>infoAssetCH</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory/>\r\n   <functions>\r\n    <function>\r\n     <name>Bank_send_accountInterest</name>\r\n     <description></description>\r\n     <currentState>start_Bank</currentState>\r\n     <nextState>Bank_start_credit_market_role</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>accountInterest</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings_payout</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>current_shares_outstanding</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Stock</type>\r\n     <name>stock</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Firm_send_info</name>\r\n     <description></