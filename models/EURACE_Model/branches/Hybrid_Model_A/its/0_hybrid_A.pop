S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Consumption_Goods_Market</name>\r\n <version>post-Oxford</version>\r\n <description></description>\r\n <models/>\r\n <environment>\r\n  <constants/>\r\n  <functionFiles>\r\n   <file>Firm_Producer_Functions.c</file>\r\n   <file>Household_Consumer_Functions.c</file>\r\n   <file>Mall_functions.c</file>\r\n   <file>Mall_aux_functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits/>\r\n  <dataTypes>\r\n   <dataType>\r\n    <name>consumption_goods_offer</name>\r\n    <description>Used by household and mall to hold consumption goods offers</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quality</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>price</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>consumption_request</name>\r\n    <description>Used by  mall to hold demand</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>worker_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>consumer_region_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>firm_id</name>\r\n      <description></description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>quantity</name>\r\n      <description></description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>mall_info</name>\r\n    <description>Used by firm to hold information about sales in malls</description>\r\n    <variables>\r\n     <variabl