n>\r\n     <name>Eurostat_idle</name>\r\n     <description></description>\r\n     <currentState>01</currentState>\r\n     <nextState>Eurostat_Start_Monthly_Loop</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <not>\r\n        <time>\r\n         <period>monthly</period>\r\n         <phase>0</phase>\r\n        </time>\r\n       </not>\r\n      </lhs>\r\n      <op>OR</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>0</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_calculate_data</name>\r\n     <description>Eurostat calculates the global and regional macro data based on the received micro data</description>\r\n     <currentState>01</currentState>\r\n     <nextState>Eurostat_Start_Monthly_Loop</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>1</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>firm_send_data</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>household_send_data</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>mall_data</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_store_history_monthly</name>\r\n     <description>Eurostat stores monthly macro data in its history struct.</description>\r\n     <currentState>Eurostat_Start_Monthly_Loop</currentState>\r\n     <nextState>02</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>1</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_compute_growth_rates_monthly</name>\r\n     <description>Eurostat computes monthly growth rates for macro data stored in its monthly history struct.</description>\r\n     <currentState>02</currentState>\r\n     <nextState>Eurostat_Start_Quarterly_Loop</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_idle</name>\r\n     <description>Eurostat does nothing</description>\r\n     <currentState>Eurostat_Start_Monthly_Loop</currentState>\r\n     <nextState>Eurostat_Start_Quarterly_Loop</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <not>\r\n        <time>\r\n         <period>monthly</period>\r\n         <phase>0</phase>\r\n        </time>\r\n       </not>\r\n      </lhs>\r\n      <op>OR</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>0</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_store_history_quarterly</name>\r\n     <description>Eurostat stores quarterly macro data in its history struct.</description>\r\n     <currentState>Eurostat_Start_Quarterly_Loop</currentState>\r\n     <nextState>04</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>quarterly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>1</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_compute_growth_rates_quarterly</name>\r\n     <description>Eurostat computes quarterly growth rates for macro data stored in its quarterly history struct.</description>\r\n     <currentState>04</currentState>\r\n     <nextState>05</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_measure_recession</name>\r\n     <description>Eurostat measures the start or end of recessions.</description>\r\n     <currentState>05</currentState>\r\n     <nextState>Eurostat_Start_EndOfYear_Loop</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_idle</name>\r\n     <description>Eurostat does nothing</description>\r\n     <currentState>Eurostat_Start_Quarterly_Loop</currentState>\r\n     <nextState>Eurostat_Start_EndOfYear_Loop</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <not>\r\n        <time>\r\n         <period>quarterly</period>\r\n         <phase>0</phase>\r\n        </time>\r\n       </not>\r\n      </lhs>\r\n      <op>OR</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>0</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_idle</name>\r\n     <description>Eurostat idle</description>\r\n     <currentState>Eurostat_Start_EndOfYear_Loop</currentState>\r\n     <nextState>Eurostat_check_flows</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>yearly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eu