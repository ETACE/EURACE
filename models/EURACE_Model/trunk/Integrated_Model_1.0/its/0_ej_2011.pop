S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Credit_Market</name>\r\n <version>Aix, 10/02/2009</version>\r\n <description></description>\r\n <models/>\r\n <environment>\r\n  <constants/>\r\n  <functionFiles>\r\n   <file>Bank_Credit_Functions.c</file>\r\n   <file>Firm_Credit_Functions.c</file>\r\n   <file>Central_Bank_Functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits>\r\n   <timeUnit>\r\n    <name>daily</name>\r\n    <unit>iteration</unit>\r\n    <period>1</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>weekly</name>\r\n    <unit>daily</unit>\r\n    <period>5</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>monthly</name>\r\n    <unit>weekly</unit>\r\n    <period>4</period>\r\n   </timeUnit>\r\n  </timeUnits>\r\n  <dataTypes>\r\n   <dataType>\r\n    <name>potential_lender</name>\r\n    <description>Collecting the name of an active bank found by firm when entering its credit market role.</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>bank_name</name>\r\n      <description>Name of Bank</description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>contacted</name>\r\n      <description>If the bank has been contacted (1) or not (0).</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>account_item</name>\r\n    <description></description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description>Id of the account\'s holder.</description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>payment_account</name>\r\n      <description>Value in the account.</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>potential_lender_array</type>\r\n     <name>set_of_lenders</name>\r\n     <description>Array of structs (potential lender) holding names of (found) active banks.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>number_of_banks_asked</name>\r\n     <description>Number of banks where the firm has asked for loans.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Firm_ask_loan</name>\r\n     <description>Firm contacts banks asking for a loan and communicating its balance sheet.</description>\r\n     <currentState>Firm_Start_Credit_Role</currentState>\r\n     <nextState>Firm_Credit_02</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.external_financial_needs</value>\r\n      </lhs>\r\n      <op>GT</op>\r\n      <rhs>\r\n       <value>0.0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>bank_identity</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_request</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_get_loan</name>\r\n     <description>Firm gets the money from banks, adds the loan to its liabilities and register all the loan features (VaR, interest rate).</description>\r\n     <currentState>Firm_Credit_02</currentState>\r\n     <nextState>Firm_End_Credit_Role</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_conditions</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.firm_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_acceptance</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash</name>\r\n     <description>Bank\'s reserves.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_credit</name>\r\n     <description>Sum of outstanding loans.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ECB_debt_cum_monthly</name>\r\n     <description>cumulated sum of ECB debt on a monthly basis</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description>Total assets minus total liabilities (or cumulated profits).</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_debt</name>\r\n     <description>Stock of liquidity borrowed from Central Bank.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>alfa</name>\r\n     <description>Basel II regulatory parameter</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>credit_rationing</name>\r\n     <description>number of firms rationed in one month</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>value_at_risk</name>\r\n     <description>Total value at risk of bank\'s loan portfolio.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>deposits_rate</name>\r\n     <description>Rate on deposits.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_gamma[2]</name>\r\n     <description>Behavioural parameter used to set interest rates.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>profits[2]</name>\r\n     <description>Current and past profits.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_lambda</name>\r\n     <description>Exogenous parameter regulating the speed of adjustment of BANK_GAMMA parameter.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_interest_rate</name>\r\n     <description>Base interest rate charged by Central Bank.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_dividend_rate</name>\r\n     <description>Percentage of bank\'s dividends on profits.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_corporate</name>\r\n     <description>Government tax rate on corporate profits.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>current_shares_outstanding</name>\r\n     <description>Number of issued shares.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>debt_period</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>loan_request_message_found</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>day_of_month_to_act</name>\r\n     <description>Day of the month to act for bank accounting function.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>deposits</name>\r\n     <description>Total end-of-the-day deposits.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_loan_installments</name>\r\n     <description>Installment amount received by firms.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_interest_payments</name>\r\n     <description>Interests paid by firms.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_loan_issues</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_interest_payment</name>\r\n     <description>Interests bank pays to Central Bank.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_dividends</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>taxes</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend_per_share</name>\r\n     <description>Dividends earned by one share.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>current_share_price</name>\r\n     <description>Market price of bank\'s shares.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Bank_read_policy_rate</name>\r\n     <description>The bank reads policy rate and updated risk free rate</description>\r\n     <currentState>Bank_update_policy_rate</currentState>\r\n     <nextState>Bank_start_credit_market_role</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>policy_rate</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description>The bank does not update risk free rate</description>\r\n     <currentState>Bank_update_policy_rate</currentState>\r\n     <nextState>Bank_start_credit_market_role</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_communicate_identity</name>\r\n     <description>Active banks communicate their identity to firms at the opening of the credit market.</description>\r\n     <currentState>Bank_start_credit_market_role</currentState>\r\n     <nextState>Bank_01</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_identity</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_send_dividend_payment</name>\r\n     <description>Function to send the dividend_per_share message to households.</description>\r\n     <currentState>Bank_01</currentState>\r\n     <nextState>Bank_01b</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>dividend_per_share</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_set_quantities_zero</name>\r\n     <description></description>\r\n     <currentState>Bank_01</currentState>\r\n     <nextState>Bank_01b</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_send_accountInterest</name>\r\n     <description>Bank communicates the interest rate on deposits.</description>\r\n     <currentState>Bank_01b</currentState>\r\n     <nextState>Bank_02</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>accountInterest</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_decide_credit_conditions</name>\r\n     <description>Bank sets loan conditions for the applying firms.</description>\r\n     <currentState>Bank_02</currentState>\r\n     <nextState>Bank_03</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_request</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_conditions</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_give_loan</name>\r\n     <description>Bank accounts the feature of the granted loan and updates its portfolio.</description>\r\n     <currentState>Bank_03</currentState>\r\n     <nextState>Bank_04</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_acceptance</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_receive_installment</name>\r\n     <description>Bank receives periodic interests and installments from client firms.</description>\r\n     <currentState>Bank_04</currentState>\r\n     <nextState>Bank_05</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>installment</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>bankruptcy</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_account_update_deposits</name>\r\n     <description>Bank updates its deposits and if needed reduces its exposition towards Central Bank.</description>\r\n     <currentState>Bank_05</currentState>\r\n     <nextState>Bank_06</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>bank_account_update</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_accounting</name>\r\n     <description>Balance sheet updates are executed.</description>\r\n     <currentState>Bank_06</currentState>\r\n     <nextState>Bank_07</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>0</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_interest_payment</messageName>\r\n      </output>\r\n      <output>\r\n       <messageName>tax_payment</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_idle</name>\r\n     <description></description>\r\n     <currentState>Bank_06</currentState>\r\n     <nextState>Bank_07</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_update_ecb_account</name>\r\n     <description>Bank sends a message to ECB with its cash (payment account of the Bank at the ECB).</description>\r\n     <currentState>Bank_07</currentState>\r\n     <nextState>end_Bank</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_to_central_bank_account_update</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Central_Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_interest_rate</name>\r\n     <description>Base rate of the Central Bank.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description>The ECB equity is the account to collect the interest payments from the banks and the government bond interest rate payments. The equity of ECB is paid out to all governments as dividends, in proportion to their population.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend_payment</name>\r\n     <description>The equity of ECB is paid out to all governments as dividends, in proportion to their population.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>fiat_money</name>\r\n     <description>This is the total fiat money.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>fiat_money_banks</name>\r\n     <description>This is the sum over banks\' ECB_debt, on the ECB\'s balance sheet, here named as a CB liability.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_deposits</name>\r\n     <description>Total sum of deposits by governments and banks at the ECB.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>account_item_array</type>\r\n     <name>accounts_banks</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>account_item_array</type>\r\n     <name>accounts_govs</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash</name>\r\n     <description>CB liquid asset which takes into account interest payments to CB from banks and (in the future) from govs.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Central_Bank_read_account_update</name>\r\n     <description>Central Bank updates accounts of banks and Government.</description>\r\n     <currentState>00</currentState>\r\n     <nextState>end_Central_Bank</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>bank_to_central_bank_account_update</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>gov_to_central_bank_account_update</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>bank_interest_payment</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Central_Bank_monetary_policy</name>\r\n     <description>Central bank implements monetary policy.</description>\r\n     <currentState>start_Central_Bank</currentState>\r\n     <nextState>CB_market_operations</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>eurostat_send_macrodata</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>policy_rate</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description>Central bank does not implement monetary policy.</description>\r\n     <currentState>start_Central_Bank</currentState>\r\n     <nextState>CB_market_operations</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Central_Bank_read_fiat_money_requests</name>\r\n     <description>Function read the requests for fiat money sent by governments.</description>\r\n     <currentState>CB_financial_operations_end</currentState>\r\n     <nextState>00</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>0</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>request_fiat_money</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>info_new_bonds_amount</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description></description>\r\n     <currentState>CB_financial_operations_end</currentState>\r\n     <nextState>00</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>policy_rate</name>\r\n   <description>Message sent by CB to communicate the policy rate.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>policy_rate_value</name>\r\n     <description>rate value</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>bank_identity</name>\r\n   <description>Message added by bank to communicate its existence to firms.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description>name of the bank</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>loan_request</name>\r\n   <description>Message added by firm to demand credit with bank_id, with financial info of applying firm.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_debt</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>external_financial_needs</name>\r\n     <description></description>\r\n   