ouble</type>\r\n     <name>specific_skill_2</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>specific_skill_3</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>specific_skill_4</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>specific_skill_5</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>productivity_progress</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cpi</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>data_for_government</name>\r\n   <description>Sent by Eurostat to governments. Includes the region_id and gdp for the region. Used by gov to compute its GDP. Also contains the mean wage for the region.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>region_id</name>\r\n     <description>Region to which the data applies.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>gdp</name>\r\n     <description>GDP in the region.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>mean_wage</name>\r\n     <description>Mean wage in the region.</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>eurostat_send_macrodata</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>inflation</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>gdp</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\