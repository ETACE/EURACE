>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_store_history_quarterly</name>\r\n     <description>Eurostat stores quarterly macro data in its history struct.</description>\r\n     <currentState>Eurostat_Start_Quarterly_Loop</currentState>\r\n     <nextState>04</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>quarterly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>1</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_compute_growth_rates_quarterly</name>\r\n     <description>Eurostat computes quarterly growth rates for macro data stored in its quarterly history struct.</description>\r\n     <currentState>04</currentState>\r\n     <nextState>05</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_measure_recession</name>\r\n     <description>Eurostat measures the start or end of recessions.</description>\r\n     <currentState>05</currentState>\r\n     <nextState>Eurostat_Start_EndOfYear_Loop</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_idle</name>\r\n     <description>Eurostat does nothing</description>\r\n     <currentState>Eurostat_Start_Quarterly_Loop</currentState>\r\n     <nextState>Eurostat_Start_EndOfYear_Loop</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <not>\r\n        <time>\r\n         <period>quarterly</period>\r\n         <phase>0</phase>\r\n        </time>\r\n       </not>\r\n      </lhs>\r\n      <op>OR</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>a.switch_datastorage</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>0</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_idle</name>\r\n     <description>Eurostat idle</description>\r\n     <currentState>Eurostat_Start_EndOfYear_Loop</currentState>\r\n     <nextState>end_Eurostat</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>yearly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description></description>\r\n     <currentState>Eurostat_Start_EndOfYear_Loop</currentState>\r\n     <nextState>end_Eurostat</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>yearly</period>\r\n       <phase>0</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Eurostat_initialization</name>\r\n     <description>Initialization function for Eurostat.</description>\r\n     <currentState>start_Eurostat</currentState>\r\n     <nextState>00</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>millionyearly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description>Idle for initialization function.</description>\r\n     <currentState>start_Eurostat</currentState>\r\n     <nextState>00</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>millionyearly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_income</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_expenses</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_assets</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_liabilities</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>idle</name>\r\n     <description></description>\r\n     <currentState>start_Bank</currentState>\r\n     <nextState>Bank_update_policy_rate</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>1</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_read_policy_announcements</name>\r\n     <description></description>\r\n     <currentState>start_Bank</currentState>\r\n     <nextState>Bank_update_policy_rate</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>1</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>policy_announcement</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.gov_id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.gov_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Central_Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bond_holdings_value</name>\r\n     <description>Total monetary amount of the ECBs government bond holdings.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>nr_gov_bonds<