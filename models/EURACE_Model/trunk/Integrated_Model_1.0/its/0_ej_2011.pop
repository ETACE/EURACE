ption>Region ID to which the agent is associated.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>partition_id</name>\r\n     <description>Partition ID used for parallelization.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions/>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Central_Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description>Agent ID.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>region_id</name>\r\n     <description>Region ID to which the agent is associated.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>partition_id</name>\r\n     <description>Partition ID used for parallelization.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions/>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Clearinghouse</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description>Agent ID.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>region_id</name>\r\n     <description>Region ID to which the agent is associated.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>partition_id</name>\r\n     <description>Partition ID used for parallelization.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions/>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>bank_account_update</name>\r\n   <description>Sent by agents to inform their bank about the current payment account balance.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description>Sending agent ID.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description>Receiving bank agent ID.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description>Current account value.</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>bank_to_central_bank_account_update</name>\r\n   <description>Banks send account information to central bank</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description>Sending agent ID.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description>Current account value.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_debt</name>\r\n     <description>Debt of the bank to the Central Bank</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>gov_to_central_bank_account_update</name>\r\n   <description>Governments send account information to central bank</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description>Sending agent ID.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>payment_account</name>\r\n     <description>Current account value.</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n </messages>\r\n</xmodel>\r\n'
