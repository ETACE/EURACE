s>\r\n  <message>\r\n   <name>order</name>\r\n   <description>order posted by the Household</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>trader_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>limit_price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>order_status</name>\r\n   <description>order status posted by clearing house</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>trader_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>infoAssetCH</name>\r\n   <description>message posted by the Clearing House </description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>info_firm</name>\r\n   <description>message posted by the