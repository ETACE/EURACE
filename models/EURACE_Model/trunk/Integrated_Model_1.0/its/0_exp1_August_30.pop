S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Investment_Goods_Market</name>\r\n <version>pre-Genoa</version>\r\n <description></description>\r\n <models/>\r\n <environment>\r\n  <constants/>\r\n  <functionFiles>\r\n   <file>IGfirm_functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits/>\r\n  <dataTypes/>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>IGFirm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>productivity</name>\r\n     <description>Current best practice technology of the IG firm.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_price</name>\r\n     <description>Price of the capital good.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>revenue_per_day</name>\r\n     <description>Daily revenue.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_corporate</name>\r\n     <description>Corporate tax rate.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_vat</name>\r\n     <description>VAT tax rate.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_payment</name>\r\n     <description>Sum of taxes paid.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>net_profit</name>\r\n     <description>Net profits: earnings after tax payment.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>outstanding_shares</name>\r\n     <description>Number of outstanding shares</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>current_dividend_per_share</name>\r\n     <description>Dividends per share</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cum_revenues</name>\r\n     <description>This counts the monthly revenue.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cum_revenue_last_month</name>\r\n     <description>Monthly revenue of the last month</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cum_net_profits</name>\r\n     <description>Sum of net profits of the last periods.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>last_net_profits</name>\r\n     <description>Array of the planned production quantities in the past</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend_payment</name>\r\n     <description>Dividends paid.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>IGFirm_update_productivity_price</name>\r\n     <description></description>\r\n     <currentState>Start_IGFirm_Productivity</currentState>\r\n     <nextState>01</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>a.day_of_month_to_act</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_idle</name>\r\n     <description></description>\r\n     <currentState>Start_IGFirm_Productivity</currentState>\r\n     <nextState>01</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>a.day_of_month_to_act</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_send_quality_price_info</name>\r\n     <description></description>\r\n     <currentState>01</currentState>\r\n     <nextState>02</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>productivity</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_send_capital_good</name>\r\n     <description></description>\r\n     <currentState>02</currentState>\r\n     <nextState>03</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>capital_good_request</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>capital_good_delivery</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_receive_payment</name>\r\n     <description></description>\r\n     <currentState>03</currentState>\r\n     <nextState>04</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>pay_capital_goods</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_account_update</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_pay_taxes</name>\r\n     <description></description>\r\n     <currentState>04</currentState>\r\n     <nextState>04b</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>a.day_of_month_to_act</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>tax_payment</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_dividend_payment</name>\r\n     <description></description>\r\n     <currentState>04b</currentState>\r\n     <nextState>end_IGFirm</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>a.day_of_month_to_act</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>dividend_per_share</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>IGFirm_idle</name>\r\n     <description></description>\r\n     <currentState>04</currentState>\r\n     <nextState>end_IGFirm</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>a.day_of_month_to_act</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>productivity</name>\r\n   <description>Sent by IG firm: Contains information about price and quality of the investment good.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>IGfirm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cap_productivity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cap_good_price</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>capital_good_request</name>\r\n   <description>Sent by firms: Contains the capital goods demand.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_demand</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>capital_good_delivery</name>\r\n   <description>Sent by IG firm: Delivery of the investment good.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_delivery_volume</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>productivity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_good_price</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>dividend</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>pay_capital_goods</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>capital_bill</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n </messages>\r\n</xmodel>\r\n'
