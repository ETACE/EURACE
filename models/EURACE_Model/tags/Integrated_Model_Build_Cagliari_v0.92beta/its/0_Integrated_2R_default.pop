S'<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">\r\n<xmodel version="2" >\r\n <name>Credit_Market</name>\r\n <version>Aix, 10/02/2009</version>\r\n <description></description>\r\n <models/>\r\n <environment>\r\n  <constants/>\r\n  <functionFiles>\r\n   <file>Bank_Credit_Functions.c</file>\r\n   <file>Firm_Credit_Functions.c</file>\r\n   <file>Central_Bank_Functions.c</file>\r\n  </functionFiles>\r\n  <timeUnits>\r\n   <timeUnit>\r\n    <name>daily</name>\r\n    <unit>iteration</unit>\r\n    <period>1</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>weekly</name>\r\n    <unit>daily</unit>\r\n    <period>5</period>\r\n   </timeUnit>\r\n   <timeUnit>\r\n    <name>monthly</name>\r\n    <unit>weekly</unit>\r\n    <period>4</period>\r\n   </timeUnit>\r\n  </timeUnits>\r\n  <dataTypes>\r\n   <dataType>\r\n    <name>potential_lender</name>\r\n    <description>Collecing the name of an active bank found by firm when entering its credit market role</description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>bank_name</name>\r\n      <description>Name of Bank</description>\r\n     </variable>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>contacted</name>\r\n      <description>if the bank has been contacted (1) or not (0)</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n   <dataType>\r\n    <name>account_item</name>\r\n    <description></description>\r\n    <variables>\r\n     <variable>\r\n      <type>int</type>\r\n      <name>id</name>\r\n      <description>id of the account\'s holder</description>\r\n     </variable>\r\n     <variable>\r\n      <type>double</type>\r\n      <name>payment_account</name>\r\n      <description>value in the account</description>\r\n     </variable>\r\n    </variables>\r\n   </dataType>\r\n  </dataTypes>\r\n </environment>\r\n <agents>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>potential_lender_array</type>\r\n     <name>set_of_lenders</name>\r\n     <description>Array of structs (potential lender) holding names of (found) active banks</description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>number_of_banks_asked</name>\r\n     <description>Number of banks where the firm has asked for loans</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Firm_ask_loan</name>\r\n     <description></description>\r\n     <currentState>Firm_Start_Credit_Role</currentState>\r\n     <nextState>Firm_Credit_02</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.external_financial_needs</value>\r\n      </lhs>\r\n      <op>GT</op>\r\n      <rhs>\r\n       <value>0.0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>bank_identity</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_request</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_get_loan</name>\r\n     <description></description>\r\n     <currentState>Firm_Credit_02</currentState>\r\n     <nextState>Firm_End_Credit_Role</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_conditions</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.firm_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_acceptance</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_credit</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_debt</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>alfa</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>value_at_risk</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_gamma[2]</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>profits[2]</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_lambda</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_interest_rate</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_dividend_rate</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>tax_rate_corporate</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>current_shares_outstanding</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>debt_period</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>loan_request_message_found</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>day_of_month_to_act</name>\r\n     <description>Day of the month to act for bank accounting function</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>deposits</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_loan_installments</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_interest_payments</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>firm_loan_issues</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_interest_payment</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend_payment</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>taxes</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Bank_communicate_identity</name>\r\n     <description></description>\r\n     <currentState>Bank_start_credit_market_role</currentState>\r\n     <nextState>Bank_02</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_identity</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_decide_credit_conditions</name>\r\n     <description></description>\r\n     <currentState>Bank_02</currentState>\r\n     <nextState>Bank_03</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_request</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>loan_conditions</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_give_loan</name>\r\n     <description></description>\r\n     <currentState>Bank_03</currentState>\r\n     <nextState>Bank_04</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>loan_acceptance</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_receive_installment</name>\r\n     <description></description>\r\n     <currentState>Bank_04</currentState>\r\n     <nextState>Bank_05</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>installment</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n      <input>\r\n       <messageName>bankruptcy</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_account_update_deposits</name>\r\n     <description></description>\r\n     <currentState>Bank_05</currentState>\r\n     <nextState>Bank_06</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>bank_account_update</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.bank_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>central_bank_account_update</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_accounting</name>\r\n     <description></description>\r\n     <currentState>Bank_06</currentState>\r\n     <nextState>Bank_07</nextState>\r\n     <condition>\r\n      <time>\r\n       <period>monthly</period>\r\n       <phase>a.day_of_month_to_act</phase>\r\n      </time>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_interest_payment</messageName>\r\n      </output>\r\n      <output>\r\n       <messageName>tax_payment</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_idle</name>\r\n     <description></description>\r\n     <currentState>Bank_06</currentState>\r\n     <nextState>end_Bank_cycle</nextState>\r\n     <condition>\r\n      <not>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>a.day_of_month_to_act</phase>\r\n       </time>\r\n      </not>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Bank_send_data_to_Eurostat</name>\r\n     <description></description>\r\n     <currentState>Bank_07</currentState>\r\n     <nextState>end_Bank_cycle</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>SWITCH_STOCK_CONSISTENCY_CHECK</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>1</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>bank_balance_sheet</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Bank_idle</name>\r\n     <description></description>\r\n     <currentState>Bank_07</currentState>\r\n     <nextState>end_Bank_cycle</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <not>\r\n        <time>\r\n         <period>monthly</period>\r\n         <phase>0</phase>\r\n        </time>\r\n       </not>\r\n      </lhs>\r\n      <op>OR</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>SWITCH_STOCK_CONSISTENCY_CHECK</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>0</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Central_Bank</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>account_item_array</type>\r\n     <name>accounts</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>ecb_interest_rate</name>\r\n     <description>Base rate of the Central Bank</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description>The ECB equity is the account to collect the interest payments from the banks and the government bond interest rate payments.&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;&#xd;\r\nThe equity of ECB is paid out to all governments as dividends, in proportion to their population.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend_payment</name>\r\n     <description>The equity of ECB is paid out to all governments as dividends, in proportion to their population.</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_ecb_debt</name>\r\n     <description>This is the sum of all the ECB_debts of the banks, on the ECB\'s balance sheet.</description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Central_Bank_read_account_update</name>\r\n     <description></description>\r\n     <currentState>00</currentState>\r\n     <nextState>01</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>central_bank_account_update</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Central_Bank_send_data_to_Eurostat</name>\r\n     <description></description>\r\n     <currentState>01</currentState>\r\n     <nextState>end_Central_Bank</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <time>\r\n        <period>monthly</period>\r\n        <phase>0</phase>\r\n       </time>\r\n      </lhs>\r\n      <op>AND</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>SWITCH_STOCK_CONSISTENCY_CHECK</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>1</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>ecb_balance_sheet</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>idle</name>\r\n     <description></description>\r\n     <currentState>01</currentState>\r\n     <nextState>end_Central_Bank</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <not>\r\n        <time>\r\n         <period>monthly</period>\r\n         <phase>0</phase>\r\n        </time>\r\n       </not>\r\n      </lhs>\r\n      <op>OR</op>\r\n      <rhs>\r\n       <lhs>\r\n        <value>SWITCH_STOCK_CONSISTENCY_CHECK</value>\r\n       </lhs>\r\n       <op>EQ</op>\r\n       <rhs>\r\n        <value>0</value>\r\n       </rhs>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>bank_identity</name>\r\n   <description>Message added by bank to communicate its existence to firms.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description>name of the bank</description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>loan_request</name>\r\n   <description>Message added by firm to demand credit with bank_id, with financial info of applying firm.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>total_debt</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>external_financial_needs</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>loan_conditions</name>\r\n   <description> Message added by bank to offer credit; contains the interest rate, the amount of offered credit, and the value_at_risk.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>firm_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>proposed_interest_rate</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>amount_offered_credit</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>value_at_risk</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>loan_acceptance</name>\r\n   <description>Message added by firm to accept a loan with bank_id, for the amount credit_taken and VAR. The bank does not need to know the firm_id.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>credit_amount_taken</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>loan_total_var</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>installment</name>\r\n   <description>Message added by firm pays installment and interest to the bank.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>installment_amount</name>\r\n     <description>installment_amount is 0 in case of bankruptcy, positive in case of No_bankruptcy</description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>interest_amount</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>var_per_installment</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>bankruptcy</name>\r\n   <description>Message added by firm to bank to signal bankruptcy.</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bad_debt</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>credit_refunded</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>residual_var</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>BCE_return</name>\r\n   <description></description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bce_debt</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>bank_interest_payment</name>\r\n   <description>Interests paid by banks to ecb</description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>bank_interest_amount</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n </messages>\r\n</xmodel>\r\n'
