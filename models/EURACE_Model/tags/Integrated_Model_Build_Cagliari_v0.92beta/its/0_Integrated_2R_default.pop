ription>\r\n    </variable>\r\n    <variable>\r\n     <type>Belief_array</type>\r\n     <name>beliefs</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order_array</type>\r\n     <name>pendingOrders</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Asset_array</type>\r\n     <name>assetsowned</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetWeights</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double_array</type>\r\n     <name>assetUtilities</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>cash_on_hand</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>forwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>backwardWindow</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bins</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>randomWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>fundamentalWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>chartistWeight</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>holdingPeriodToForwardW</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>lossaversion</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>strategy</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Household_receive_info_interest_from_bank</name>\r\n     <description></description>\r\n     <currentState>Household_Start_Financial_Market_Role</currentState>\r\n     <nextState>SELECTSTRATEGY</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>accountInterest</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_select_strategy</name>\r\n     <description></description>\r\n     <currentState>SELECTSTRATEGY</currentState>\r\n     <nextState>CHOOSE_TO_TRADE_OR_NOT</nextState>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_stock_beliefs_formation</name>\r\n     <description></description>\r\n     <currentState>CHOOSE_TO_TRADE_OR_NOT</currentState>\r\n     <nextState>BOND_BELIEF_FORMATION</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.strategy</value>\r\n      </lhs>\r\n      <op>NEQ</op>\r\n      <rhs>\r\n       <value>0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>info_firm</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_bond_beliefs_formation</name>\r\n     <description></description>\r\n     <currentState>BOND_BELIEF_FORMATION</currentState>\r\n     <nextState>SEND_ORDERS</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.strategy</value>\r\n      </lhs>\r\n      <op>NEQ</op>\r\n      <rhs>\r\n       <value>0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs>\r\n      <input>\r\n       <messageName>info_bond</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_does_not_trading</name>\r\n     <description></description>\r\n     <currentState>CHOOSE_TO_TRADE_OR_NOT</currentState>\r\n     <nextState>Household_Start_Labour_Role</nextState>\r\n     <condition>\r\n      <lhs>\r\n       <value>a.strategy</value>\r\n      </lhs>\r\n      <op>EQ</op>\r\n      <rhs>\r\n       <value>0</value>\r\n      </rhs>\r\n     </condition>\r\n     <inputs/>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Household_send_orders</name>\r\n     <description></description>\r\n     <currentState>SEND_ORDERS</currentState>\r\n     <nextState>WAITORDERSTATUS</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Household_update_its_portfolio</name>\r\n     <description></description>\r\n     <currentState>WAITORDERSTATUS</currentState>\r\n     <nextState>Household_Start_Labour_Role</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order_status</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.trader_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Clearinghouse</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>Asset_array</type>\r\n     <name>assets</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>ClearingMechanism</type>\r\n     <name>clearingmechanism</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>ClearingHouse_receive_info</name>\r\n     <description></description>\r\n     <currentState>start_Clearinghouse</currentState>\r\n     <nextState>RECEIVEDINFOSTOCK</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>info_firm</messageName>\r\n      </input>\r\n      <input>\r\n       <messageName>info_bond</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_receive_orders_and_run</name>\r\n     <description></description>\r\n     <currentState>RECEIVEDINFOSTOCK</currentState>\r\n     <nextState>COMPUTEDPRICES</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order_status</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_send_asset_information</name>\r\n     <description></description>\r\n     <currentState>COMPUTEDPRICES</currentState>\r\n     <nextState>end_Clearinghouse</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>infoAssetCH</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory/>\r\n   <functions>\r\n    <function>\r\n     <name>Bank_send_accountInterest</name>\r\n     <description></description>\r\n     <currentState>start_Bank</currentState>\r\n     <nextState>Bank_start_credit_market_role</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>accountInterest</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings_payout</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>current_shares_outstanding</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Stock</type>\r\n     <name>stock</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Firm_send_info</name>\r\n     <description></description>\r\n     <currentState>Firm_End_Macro_Data</currentState>\r\n     <nextState>Firm_Start_Producer_Role</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>info_firm</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_receive_stock_info</name>\r\n     <description></description>\r\n     <currentState>15</currentState>\r\n     <nextState>16</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>infoAssetCH</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.asset_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Government</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>Bond</type>\r\n     <name>bond</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order</type>\r\n     <name>pending_order</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Government_send_info_bond</name>\r\n     <description></description>\r\n     <currentState>Gov_Start_Bond_Market</currentState>\r\n     <nextState>GOVERNMENT_SENT_info_bond</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>info_bond</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_generate_pending_orders</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_SENT_info_bond</currentState>\r\n     <nextState>GOVERNMENT_IN_DEFICIT</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_orders_issuing</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_IN_DEFICIT</currentState>\r\n     <nextState>GOVERNMENT_SENT_ORDER</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_update_its_portfolio</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_SENT_ORDER</currentState>\r\n     <nextState>GOVERNMENT_PORTFOLIO_UPDATED</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order_status</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.trader_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_receive_info_bond</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_PORTFOLIO_UPDATED</currentState>\r\n     <nextState>01</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>infoAssetCH</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.asset_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n </agents>\r\n <messages>\r\n  <message>\r\n   <name>order</name>\r\n   <description>order posted by the Household</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>trader_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>limit_price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>order_status</name>\r\n   <description>order status posted by clearing house</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>trader_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>infoAssetCH</name>\r\n   <description>message posted by the Clearing House </description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>asset_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>price</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>quantity</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>info_firm</name>\r\n   <description>message posted by the Firm</description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>dividend</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings_payout</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Stock</type>\r\n     <name>stock</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>accountInterest</name>\r\n   <description>message posted by the Household </description>\r\n   <variables>\r\n    <variable>\r\n     <type>int</type>\r\n     <name>bank_id</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>interest</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>bankAccountUpdate</name>\r\n   <description>message posted by the Household </description>\r\n   <variables>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>account</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n  <message>\r\n   <name>info_bond</name>\r\n   <description>message posted by the Government </description>\r\n   <variables>\r\n    <variable>\r\n     <type>Bond</type>\r\n     <name>bond</name>\r\n     <description></description>\r\n    </variable>\r\n   </variables>\r\n  </message>\r\n </messages>\r\n</xmodel>\r\n'
