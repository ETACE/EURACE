e>\r\n      </input>\r\n      <input>\r\n       <messageName>info_bond</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_receive_orders_and_run</name>\r\n     <description></description>\r\n     <currentState>RECEIVEDINFOSTOCK</currentState>\r\n     <nextState>COMPUTEDPRICES</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order</messageName>\r\n      </input>\r\n     </inputs>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order_status</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>ClearingHouse_send_asset_information</name>\r\n     <description></description>\r\n     <currentState>COMPUTEDPRICES</currentState>\r\n     <nextState>end_Clearinghouse</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>infoAssetCH</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Bank</name>\r\n   <description></description>\r\n   <memory/>\r\n   <functions>\r\n    <function>\r\n     <name>Bank_send_accountInterest</name>\r\n     <description></description>\r\n     <currentState>start_Bank</currentState>\r\n     <nextState>Bank_start_credit_market_role</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>accountInterest</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Firm</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>earnings_payout</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>current_shares_outstanding</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>current_dividend_per_share</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>equity</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Stock</type>\r\n     <name>stock</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Firm_send_info</name>\r\n     <description></description>\r\n     <currentState>start_Firm</currentState>\r\n     <nextState>received_stock_info</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>info_firm</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Firm_receive_stock_info</name>\r\n     <description></description>\r\n     <currentState>received_stock_info</currentState>\r\n     <nextState>end_Firm</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>infoAssetCH</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.asset_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n   </functions>\r\n  </xagent>\r\n  <xagent>\r\n   <name>Government</name>\r\n   <description></description>\r\n   <memory>\r\n    <variable>\r\n     <type>Bond</type>\r\n     <name>bond</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>Order</type>\r\n     <name>pending_order</name>\r\n     <description></description>\r\n    </variable>\r\n    <variable>\r\n     <type>double</type>\r\n     <name>deficit</name>\r\n     <description></description>\r\n    </variable>\r\n   </memory>\r\n   <functions>\r\n    <function>\r\n     <name>Government_send_info_bond</name>\r\n     <description></description>\r\n     <currentState>start_Government</currentState>\r\n     <nextState>GOVERNMENT_SENT_info_bond</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>info_bond</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_generate_pending_orders</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_SENT_info_bond</currentState>\r\n     <nextState>GOVERNMENT_IN_DEFICIT</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_orders_issuing</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_IN_DEFICIT</currentState>\r\n     <nextState>GOVERNMENT_SENT_ORDER</nextState>\r\n     <inputs/>\r\n     <outputs>\r\n      <output>\r\n       <messageName>order</messageName>\r\n      </output>\r\n     </outputs>\r\n    </function>\r\n    <function>\r\n     <name>Government_update_its_portfolio</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_SENT_ORDER</currentState>\r\n     <nextState>GOVERNMENT_PORTFOLIO_UPDATED</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>order_status</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <value>a.id</value>\r\n        </lhs>\r\n        <op>EQ</op>\r\n        <rhs>\r\n         <value>m.trader_id</value>\r\n        </rhs>\r\n       </filter>\r\n      </input>\r\n     </inputs>\r\n     <outputs/>\r\n    </function>\r\n    <function>\r\n     <name>Government_receive_info_bond</name>\r\n     <description></description>\r\n     <currentState>GOVERNMENT_PORTFOLIO_UPDATED</currentState>\r\n     <nextState>end_Government</nextState>\r\n     <inputs>\r\n      <input>\r\n       <messageName>infoAssetCH</messageName>\r\n       <filter>\r\n        <lhs>\r\n         <va